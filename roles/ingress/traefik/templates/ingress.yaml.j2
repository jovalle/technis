apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: traefik-dashboard
  namespace: {{ ingress_controller_namespace }}
spec:
  rules:
  - host: traefik.{{ k8s_ingress_domain }}
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: traefik
            port:
              number: 9000