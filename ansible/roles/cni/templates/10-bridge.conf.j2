{
    "cniVersion": "{{ cni_version }}",
    "name": "bridge",
    "type": "bridge",
    "bridge": "cnio0",
    "isGateway": true,
    "ipMasq": true,
    "ipam": {
        "type": "host-local",
        "ranges": [
          [{"subnet": "{{ k8s_pod_cidr | default('10.200.0.0/16') }}"}]
        ],
        "routes": [{"dst": "0.0.0.0/0"}]
    }
}